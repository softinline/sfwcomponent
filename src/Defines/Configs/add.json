{
    "type":"screen",
    "layout":"sfwcomponent::backoffice.layouts.default",
    "ajax":false,
    "name":"sfwconfig",
    "url":"sfw/config",
    "components":[        
        {
            "type":"div",
            "class":"row",
            "components":[
                {
                    "type":"div",
                    "class":"col-lg-12",
                    "components":[
                        {
                            "type":"h",
                            "number":"3",
                            "text":"config_sfw"
                        }
                    ]
                }
            ]
        },
        {
            "type":"div",
            "class":"row",
            "components":[                
                {
                    "type":"div",
                    "class":"col-lg-12",
                    "components":[
                        {                                                                    
                            "type":"form",
                            "class":"",
                            "id":"frm-sfw-config-add",
                            "action":"sfw/config/add",                            
                            "redirect":"/",
                            "components":[
                                {
                                    "type":"tabs",
                                    "components":[
                                        {
                                            "type":"tab",
                                            "title":"general",
                                            "key":"general",
                                            "components":[
                                                {
                                                    "type":"div",
                                                    "class":"card",
                                                    "components":[
                                                        {
                                                            "type":"div",
                                                            "class":"card-body",
                                                            "components":[
                                                                {
                                                                    "type":"text",
                                                                    "required":true,
                                                                    "title":"config",
                                                                    "field":"config"
                                                                },
                                                                {
                                                                    "type":"file",
                                                                    "required":false,
                                                                    "title":"favicon",
                                                                    "field":"favicon"
                                                                }
                                                            ]
                                                        }
                                                    ]
                                                }
                                            ]
                                        }
                                    ]
                                },
                                {
                                    "type":"div",
                                    "class":"mt-2 mb-2",
                                    "components":[                                
                                        {
                                            "type":"button",
                                            "action":"js:sfwcomponent.submit('frm-sfw-config-add')",
                                            "title":"accept",
                                            "class":"btn btn-primary"
                                        }                                       
                                    ]
                                }
                            ]                                        
                        }
                    ]
                }
            ]
        }   
    ]
}