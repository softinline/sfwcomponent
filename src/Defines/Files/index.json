{
    "type":"screen",    
    "layout":"sfwcomponent::backoffice.layouts.default",
    "ajax":false,
    "name":"index",
    "url":"sfw/files",
    "components":[
        {
            "type":"function",
            "function":"breadcrumbIndex"
        },        
        {
            "type":"div",
            "class":"row",
            "components":[                
                {
                    "type":"div",
                    "class":"col-lg-12",
                    "components":[
                        {
                            "type":"div",
                            "class":"card",
                            "components":[
                                {
                                    "type":"div",
                                    "class":"card-body",
                                    "components":[
                                        {
                                            "type":"div",
                                            "class":"col-lg-12",
                                            "components":[
                                                {
                                                    "type":"datatable",
                                                    "class":"table",
                                                    "name":"sfw-files",
                                                    "footer":"true",
                                                    "pageLength":"50",
                                                    "url":"/sfw/files",
                                                    "selector":false,
                                                    "columns":[
                                                        {
                                                            "name":"id",
                                                            "field":"id",
                                                            "width":"1%"
                                                        },
                                                        {
                                                            "name":"table",
                                                            "field":"table"
                                                        },
                                                        {
                                                            "name":"row_id",
                                                            "field":"row_id"
                                                        },
                                                        {
                                                            "name":"file",
                                                            "field":"file"
                                                        },
                                                        {
                                                            "name":"size",
                                                            "field":"size"
                                                        },
                                                        {
                                                            "name":"created_at",
                                                            "field":"created_at"
                                                        },
                                                        {
                                                            "name":"actions",
                                                            "field":"actions",
                                                            "orderable":false,
                                                            "searchable":false
                                                        }
                                                    ]
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}